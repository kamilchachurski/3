{
    "parser": "babel-eslint",
    "parserOptions":
    {
        "ecmaFeatures":
        {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": ["react"],
    "settings":
    {
        "react":
        {
            "version": "detect"
        }
    },
    "env":
    {
        "browser": true,
        "node": true,
        "es2020": true
    }
}